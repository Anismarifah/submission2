{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": ["error", "allow"]
    }
}
